<?xml version="1.0" encoding="UTF-8"?>
<petrinet:PetriNet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petrinet="http://petrinet"
    name="twoActivities">
  <petriNetElements
      xsi:type="petrinet:Place"
      name="A1_ready"
      successors="//@petriNetElements.1"
      numJetons="1"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.0"
      destination="//@petriNetElements.2"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Transition"
      name="A1_start"
      predecessors="//@petriNetElements.1"
      successors="//@petriNetElements.3"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.2"
      destination="//@petriNetElements.4"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Place"
      name="A1_started"
      predecessors="//@petriNetElements.3"
      successors="//@petriNetElements.5"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.4"
      destination="//@petriNetElements.6"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Transition"
      name="A1_finish"
      predecessors="//@petriNetElements.5"
      successors="//@petriNetElements.7"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.6"
      destination="//@petriNetElements.8"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Place"
      name="A1_finished"
      predecessors="//@petriNetElements.7"
      successors="//@petriNetElements.9"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.8"
      destination="//@petriNetElements.12"/>
  <petriNetElements
      xsi:type="petrinet:Place"
      name="A2_ready"
      successors="//@petriNetElements.11"
      numJetons="1"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.10"
      destination="//@petriNetElements.12"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Transition"
      name="A2_start"
      predecessors="//@petriNetElements.11 //@petriNetElements.9"
      successors="//@petriNetElements.13"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.12"
      destination="//@petriNetElements.14"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Place"
      name="A2_started"
      predecessors="//@petriNetElements.13"
      successors="//@petriNetElements.15"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.14"
      destination="//@petriNetElements.16"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Transition"
      name="A2_finish"
      predecessors="//@petriNetElements.15"
      successors="//@petriNetElements.17"/>
  <petriNetElements
      xsi:type="petrinet:Arc"
      source="//@petriNetElements.16"
      destination="//@petriNetElements.18"
      weight="1"/>
  <petriNetElements
      xsi:type="petrinet:Place"
      name="A2_finished"
      predecessors="//@petriNetElements.17"/>
</petrinet:PetriNet>
