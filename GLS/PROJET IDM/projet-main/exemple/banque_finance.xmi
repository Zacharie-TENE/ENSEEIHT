<?xml version="1.0" encoding="UTF-8"?>
<tableModel:Tableau
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:tableModel="https://www.exemple.org/tableModel"
    name="banque finance"
    colonneDeLignes="//@colonnes.0">
  <colonnes
      name="Date"
      dataType="float"
      identifier="banque.finance.date"/>
  <colonnes
      name="Ouverture"
      dataType="float"
      identifier="banque.finance.ouverture">
    <colonneType
        xsi:type="tableModel:ImportationTableau"
        identifiantColonne="banque.finance.raw.open">
      <tableau
          href="banque_raw_finance.xmi#/"/>
    </colonneType>
  </colonnes>
  <colonnes
      name="Min"
      dataType="float"
      identifier="banque.finance.min">
    <colonneType
        xsi:type="tableModel:ImportationTableau"
        identifiantColonne="banque.finance.raw.low">
      <tableau
          href="banque_raw_finance.xmi#/"/>
    </colonneType>
    <contrainte>DOIT_ETRE_NEGATIF</contrainte>
  </colonnes>
  <colonnes
      name="Max"
      dataType="float"
      identifier="banque.finance.max">
    <colonneType
        xsi:type="tableModel:ImportationTableau"
        identifiantColonne="banque.finance.raw.high">
      <tableau
          href="banque_raw_finance.xmi#/"/>
    </colonneType>
  </colonnes>
  <colonnes
      name="Fermeture"
      dataType="float"
      identifier="banque.finance.fermeture">
    <colonneType
        xsi:type="tableModel:ImportationTableau"
        identifiantColonne="banque.finance.raw.close">
      <tableau
          href="banque_raw_finance.xmi#/"/>
    </colonneType>
  </colonnes>
  <colonnes
      name="Volume"
      dataType="float"
      identifier="banque.finance.volume">
    <colonneType
        xsi:type="tableModel:ImportationTableau"
        identifiantColonne="banque.finance.raw.volume">
      <tableau
          href="banque_raw_finance.xmi#/"/>
    </colonneType>
  </colonnes>
  <colonnes
      name="Moyenne"
      dataType="float"
      identifier="banque.finance.moyenne">
    <colonneType
        xsi:type="tableModel:ImportationAlgo"
        ColonneIN="//@colonnes.2 //@colonnes.3">
      <algorithm
          href="calcul_moyenne.xmi#/"/>
    </colonneType>
  </colonnes>
  <colonnes
      name="Variation"
      dataType="float"
      identifier="banque.finance.variation">
    <colonneType
        xsi:type="tableModel:ImportationAlgo"
        ColonneIN="//@colonnes.4 //@colonnes.1">
      <algorithm
          href="calcul_variation.xmi#/"/>
    </colonneType>
  </colonnes>
</tableModel:Tableau>
